{
    "manifest_version": 3,
    "name": "Google Drive ReOrganizer",
    "description": "improves my least favorite parts of google",
    "version": "0.1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
      "default_popup": "hello.html"
    },
    "content_scripts": [
        {
        "js": ["content.js"],
        "matches": ["https://drive.google.com/drive/my-drive"]
        }
    ],
    "background": [
        {
        "service_worker": "serviceworker.js",
        "type": "module"
        }
    ]
}